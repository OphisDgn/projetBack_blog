{% extends 'base.html.twig' %}

{% block body %}


<div class="list_container">
    <div class="row">
        {% for art in articles %}
            <div class="col-lg-4">
                <div class="card">
                    <!--img src/-->
                    <div class="card-body">
                        <div class="card-title">{{ art.title }}</div>
                        <div class="card-text">{{ art.content }}</div>
                    </div>
                </div>
            </div>
        {% endfor %}
    </div>
</div>
{% endblock %}
